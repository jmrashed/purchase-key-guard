{
    "name": "jmrashed/purchase-key-guard",
    "description": "A Laravel package to protect usage via a purchase key.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rashed Zaman",
            "email": "jmrashed@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "brian2694/laravel-toastr": "^5.59",
        "guzzlehttp/guzzle": "^7.9"
    },
    "autoload": {
        "psr-4": {
            "Jmrashed\\PurchaseKeyGuard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jmrashed\\PurchaseKeyGuard\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jmrashed\\PurchaseKeyGuard\\Providers\\PurchaseKeyGuardServiceProvider"
            ],
            "aliases": {
                "PurchaseKeyGuard": "Jmrashed\\PurchaseKeyGuard\\PurchaseKeyGuard"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
